<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    @RenderSection("meta", required: false)
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    @*<meta name="viewport" content="width=device-width, initial-scale=1">*@
    <title>@ViewBag.title</title>
    <meta name="description" content="@Html.Raw(@ViewBag.des)">
    <meta name="robots" content="index,follow,noodp" />
    <meta name="author" content="vietgiapha.com" />
    <meta property="og:locale" content="vi_VN" />
    <meta property="og:type" content="article" />
    <meta property="og:url" content="@Html.Raw(ViewBag.url)" />
    <meta property="og:title" content="@Html.Raw(@ViewBag.title)" />
    <meta property="og:description" content="@Html.Raw(@ViewBag.des)" />
    <meta property="og:image" itemprop="thumbnailUrl" content="@ViewBag.image" />
    @*<meta property="fb:admins" content="100006557590947" />*@
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/font-awesome.min.css" rel="stylesheet" />
    <link href="~/Content/owl.carousel.css" rel="stylesheet" />
    <link href="~/Content/sld/css/slider-pro.min.css" rel="stylesheet" />
    <link href="~/Content/Site.css" rel="stylesheet" />
    <script src="~/Scripts/jquery-1.12.4.min.js"></script>
    <script src="~/Scripts/jquery-ui-1.12.0.min.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
    <script>
    (function (i, s, o, g, r, a, m) {
        i['GoogleAnalyticsObject'] = r; i[r] = i[r] || function () {
            (i[r].q = i[r].q || []).push(arguments)
        }, i[r].l = 1 * new Date(); a = s.createElement(o),
        m = s.getElementsByTagName(o)[0]; a.async = 1; a.src = g; m.parentNode.insertBefore(a, m)
    })(window, document, 'script', 'https://www.google-analytics.com/analytics.js', 'ga');

    ga('create', 'UA-92968790-1', 'auto');
    ga('send', 'pageview');

    </script>
</head>
<body>

    <header id="header">
        <div class="container">
            <div class="row" id="header_1">
                <div class="logo">
                    <img src="~/images/logo.png" alt="" class="img-responsive">
                    <div class="trongdong center-block hidden-xs"></div>
                </div>

            </div>
        </div>
    </header>


    <nav class="navbar navbar-default nav_xyz1">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                @Html.ActionLink("Trang chủ", "Index", "Home", new { area = "" }, new { @class = "navbar-brand", style = "color: #fff;" })
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li><a href="/Home/GiaPha">Gia phả</a></li>
                    <li><a href="/tin/gioi-thieu">Dòng họ Việt Nam</a></li>
                    <li><a href="/tin/doanh-nghiep-dong-ho">Doanh nghiệp các dòng họ</a></li>
                    <li>@Html.ActionLink("Liên hệ", "Contact", "Home")</li>
                    @Html.Partial("_LoginPartial")
                </ul>

            </div>
        </div>
    </nav>

    <div id="main">
        <div class="container main_wrapper">
            @RenderBody()
        </div>
    </div>

    <footer id="footer">
        <div class="container">
            <div class="row">
                <div class="col-md-12" style="font-size: 14px;">
                    <h3>MẠNG XÃ HỘI GIA PHẢ VIỆT NAM</h3>
                    <p>&copy; @DateTime.Now.Year - Thiết kế bởi công ty Minh Việt - Địa chỉ Duy Tân, Cầu Giấy, Hà Nội - Điện thoại: 097 977 6427 email:vnnvh80@gmail.com</p>

                </div>
            </div>
        </div>
    </footer>


    @*<div class="cdtrai hidden-xs"></div>
        <div class="cdphai hidden-xs"></div>*@


    <script src="~/Scripts/owl.carousel.js"></script>
    <script src="~/Content/sld/js/jquery.sliderPro.min.js"></script>
    <script src="~/Scripts/main.js"></script>
    @RenderSection("scripts", required: false)
</body>
</html>
